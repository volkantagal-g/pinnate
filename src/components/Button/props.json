{
  "Button": {
    "label": {
      "type": "string"
    },
    "variant": {
      "type": "select",
      "options": [
        "primary",
        "secondary",
        "tertiary"
      ]
    },
    "size": {
      "type": "select",
      "options": [
        "xs",
        "sm",
        "md",
        "lg"
      ]
    },
    "icon": {
      "type": "React.ReactNode"
    },
    "iconPosition": {
      "type": "select",
      "options": [
        "start",
        "end"
      ]
    },
    "onlyIcon": {
      "type": "boolean"
    },
    "error": {
      "type": "boolean"
    },
    "color": {
      "type": "boolean"
    },
    "permissionId": {
      "type": "string"
    }
  }
}