{
  "Modal": {
    "isOpen": {
      "type": "boolean"
    },
    "onClose": {
      "type": "() => void"
    },
    "title": {
      "type": "string"
    },
    "children": {
      "type": "React.ReactNode"
    },
    "size": {
      "type": "select",
      "options": [
        "sm",
        "md",
        "lg"
      ]
    },
    "variant": {
      "type": "select",
      "options": [
        "default",
        "success",
        "warning",
        "error"
      ]
    },
    "footerActions": {
      "type": "React.ReactNode"
    }
  }
}